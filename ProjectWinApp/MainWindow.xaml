<Window x:Class="ProjectWinApp.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ProjectWinApp"
        mc:Ignorable="d"
        Title="Project" Height="450" Width="800">
    
    <StackPanel HorizontalAlignment="Center" Width="400">
        <Label>Email</Label>
        <TextBox x:Name="tbEmail"></TextBox>
        <Label>Password</Label>
        <TextBox x:Name="tbPassword"></TextBox>
        <Button x:Name="btnLogIn" Margin="0,20,0,0" Click="LogIn">Log in</Button>
        <Button x:Name="btnLogInQuick" Margin="0,20,0,0" Click="LogInQuick">Auto log in als admin</Button>
    </StackPanel>
</Window>

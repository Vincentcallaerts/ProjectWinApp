<Page x:Class="ProjectWinApp.Overvieuw"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:ProjectWinApp"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Overvieuw">

    <DockPanel>
        <StackPanel DockPanel.Dock="Left" Width="150" Background="AliceBlue">
            <Label BorderBrush="Black" BorderThickness="2" Margin="0,0,0,10">Overvieuw</Label>
            <Button x:Name="btnAddUser">See Users</Button>
            <Button x:Name="btnAddCustomer">See Customers</Button>
            <Button x:Name="btnAddMagazijn">See Magazijns</Button>
            

            <Button x:Name="btnBlank">Empty</Button>
        </StackPanel>
        <Frame x:Name="fOvervieuw" DockPanel.Dock="Right" Width="650" Background="White" NavigationUIVisibility="Hidden"></Frame>

    </DockPanel>
</Page>
